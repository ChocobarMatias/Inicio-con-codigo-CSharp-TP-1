Proceso TP_N_1_Ejercicio_1
	Definir num1,num2,suma,product como Entero;
	Escribir "Ingresar un numero entero: ";
	
	Leer num1;
	
	Escribir "Ingresar otro numero entero: ";
	
	Leer num2;
	
	suma = num1 + num2;
	product = num1 * num2;
	
	Escribir  "La suma de los numeros enteros ingresados es: ", suma;
	
	Escribir"El producto de los numero enteros ingresados es: ", product;

	
FinProceso
