Proceso  TP_N_1_Ejercicio_4
	Definir  num1,num2,num3,num4,suma, promedio Como Entero;
	Escribir  "Ingresar Primer Numero = ";
	Leer num1;
	
	Escribir "Ingresar Segundo Numero = ";
	Leer num2;
	
	Escribir "Ingresar Tercero Numero = ";
	Leer num3;
	
	Escribir "Ingresar Cuarto Numero = ";
	Leer num4;
	
	suma = num1 + num2 + num3 + num4;
	promedio = (suma / 4);
	
	Escribir "Suma = ", suma;

	Escribir "Promedio = ", promedio;
	
FinProceso
